{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "mocha": true,
        "jest": true
    },
    "rules": {
        "import/prefer-default-export": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "max-len": ["error", 100],
        "spaced-comment": 0,
        "arrow-parens": [2, "as-needed"],
        "comma-dangle": ["error", "never"],
        "semi": [2, "never"],
        "global-require": 0,
        "react/jsx-filename-extension": 0
    }
}
